{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "chuyennhanali/sidebar-menu-block",
  "title": "NaLi Side Bar Menu Block",
  "category": "nali-blocks",
  "icon": "menu-alt3",
  "description": "NaLi Side Bar Menu Block",
  "textdomain": "nali-custom-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js",
  "supports": {
    "html": false,
    "align": ["wide", "full"],
    "anchor": true,
    "customClassName": true,
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "color": {
      "background": true,
      "text": true,
      "link": false,
      "gradients": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "fontWeight": true,
      "fontStyle": true,
      "fontFamily": true,
      "__experimentalFontFamily": true,
      "__experimentalFontWeight": true,
      "__experimentalFontStyle": true,
      "__experimentalTextTransform": true,
      "__experimentalTextDecoration": true,
      "__experimentalLetterSpacing": true,
      "__experimentalDefaultControls": {
        "fontSize": true,
        "fontWeight": true,
        "lineHeight": true
      }
    },
    "shadow": true,
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "layout": {
      "default": {
        "type": "constrained"
      }
    }
  },
  "attributes": {
    "menuItems": {
      "type": "array",
      "default": []
    },
    "menuTitle": {
      "type": "string",
      "default": "Menu Dashboard"
    },
    "showTitle": {
      "type": "boolean",
      "default": true
    },
    "backgroundColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "textColor": {
      "type": "string",
      "default": "#64748b"
    },
    "activeBackgroundColor": {
      "type": "string",
      "default": "#3b82f6"
    },
    "activeTextColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "titleBackgroundColor": {
      "type": "string",
      "default": ""
    },
    "titleTextColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "hoverBackgroundColor": {
      "type": "string",
      "default": "#f8fafc"
    },
    "hoverTextColor": {
      "type": "string",
      "default": "#3b82f6"
    }
  }
}